{
    "extends": ["eslint:recommended", "prettier", "prettier/standard"],
    "plugins": ["import", "prettier"],
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "env": {
        "node": true,
        "es6": true,
        "jest": true
    },
    "rules": {
        "space-before-function-paren": 1,
        "new-cap": 1
    }
}
